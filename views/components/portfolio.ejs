<section id="portfolio" class="">
    <!-- desktop-->
    <div class="container">
      <div class="section-title">
        <h2>Portfolio</h2>
      </div>
    </div>
    <!-- desktop -->
    <div class="container desktop">
      <div class="div">
        <div class="row ">
          <% portfolio.forEach(data => { %>
            <div class="col-lg-4 p-3 mb-3">
              <div class="card shadow mx-auto w-100" style="">
                <img class="card-img-top" src="<%= data.img %> " alt="Card image cap">
                <div class="card-body">
                  <h5 class="card-title"><%= data.title %> </h5>
                  <p class="card-text">
                    <%= data.desc %> 
                  </p>
                  <div class="row">
                    <div class="col-6">
                      <button type="button" class="btn bg-second light w-100" data-bs-toggle="modal" data-bs-target="<%= data.modalTarget %> ">
                        Details
                      </button>
                    </div>
                    <div class="col-6">
                      <a href="<%= data.link %> " target="_blank" class="btn btn-primary w-100">Live View</a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          <% }) %>
        </div>
      </div>
    </div>
